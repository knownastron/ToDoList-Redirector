{
  "manifest_version": 2,
  "name": "To-do List Redirector",
  "version": "0.01",
  "permissions": ["tabs",
                  "storage",
                  "webNavigation",
                  "http://*/",
                  "https://*/"],
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "Options"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
